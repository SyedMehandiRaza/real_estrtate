<div class="modal-overlay">
  <div class="modal">
    <div class="modal-header">
      <div class="user-info">
        <div class="avatar">
          <img src="https://i.pravatar.cc/150?img=12" alt="Shivam Gawali" />
        </div>
        <div class="user-details">
          <h2 id="sdName">Shivam Gawali</h2>
          <p class="user-role" id="sdRole">Agent</p>
        </div>
      </div>
      <div class="header-actions">
        <div class="status-dropdown">
          <div class="status-select">Active</div>
        </div>
        <button class="btn btn-edit">Edit</button>
        <button class="btn btn-remove">Remove Staff</button>
        <button class="close-btn" onclick="closeModal()">✕</button>
      </div>
    </div>

    <div class="contact-info">
      <div class="contact-item">
        <svg
          class="contact-icon"
          fill="none"
          stroke="currentColor"
          viewBox="0 0 24 24"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2"
            d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"
          />
        </svg>
        <div class="contact-details">
          <h4 id="sdPhone">+91 7485961245</h4>
          <p class="contact-label">Phone Number</p>
        </div>
      </div>

      <div class="contact-item">
        <svg
          class="contact-icon"
          fill="none"
          stroke="currentColor"
          viewBox="0 0 24 24"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2"
            d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"
          />
        </svg>
        <div class="contact-details">
          <h4 id="sdEmail">Shivamgawali@gmail.com</h4>
          <p class="contact-label">Email Address</p>
        </div>
      </div>

      <div class="contact-item">
        <svg
          class="contact-icon"
          fill="none"
          stroke="currentColor"
          viewBox="0 0 24 24"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2"
            d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"
          />
        </svg>
        <div class="contact-details">
          <div class="status-badge" id="sdStatus">
            <span class="status-dot"></span>
            Active
          </div>
          <p class="contact-label">Status</p>
        </div>
      </div>
    </div>

    <div class="modal-body">
      <div class="section">
        <h3 class="section-title">Assigned Permissions / Access</h3>
        <div class="permission-list" id="sdPermissionList">
          <div class="permission-item">
            <div class="permission-icon check">✓</div>
            Properties Management
          </div>
          <div class="permission-item">
            <div class="permission-icon check">✓</div>
            Leads & CRM
          </div>
          <div class="permission-item">
            <div class="permission-icon check">✓</div>
            Payment Tracking
          </div>
          <div class="permission-item">
            <div class="permission-icon cross">✕</div>
            Marketing
          </div>
          <div class="permission-item">
            <div class="permission-icon cross">✕</div>
            Facility Management
          </div>
        </div>
      </div>
      <div class="section-two">
        <h3 class="section-title">Assigned Property</h3>

        <div class="property-scroll">
          <% if (ownerProperties && ownerProperties.length > 0) { %> <%
          ownerProperties.forEach(property => { %>

          <div class="property-card">
            <div class="property-image">
              <img
                src="<%= property.media?.[0]?.url || 'https://via.placeholder.com/400' %>"
                alt="<%= property.title %>"
              />
            </div>

            <div class="property-info">
              <div class="ready-badge">Ready to move</div>

              <h4 class="property-name"><%= property.title %></h4>
              <p class="property-location"><%= property.address %></p>

              <div class="property-stats">
                <div class="property-stat">Active</div>
                <div class="property-stat">Property Assigned</div>

                <div>
                  <button
                    class="btn-view"
                    onclick="window.location.href='/propertydetail/<%= property.id %>'"
                  >
                    View Property
                  </button>
                </div>
              </div>
            </div>
          </div>

          <% }) %> <% } else { %>
          <p style="color: #777">No property assigned</p>
          <% } %>
        </div>

        <button class="btn-assign">Assign Property</button>
      </div>

