     <script>
function stopFormBeforeFooter() {
  const agentCard = document.querySelector('.pr-de-agent-card');
  const footer = document.querySelector('.pr-de-footer-wrapper');
  const similarListing = document.querySelector('.pr-de-wrapper-main');
  const wrapperWhole = document.querySelector('.pr-de-wrapper-whole');
  const priceTag = document.querySelector('.pr-de-price');
  
  if (!agentCard || !wrapperWhole || !priceTag) return;

  function checkPosition() {
    // Mobile check
    if (window.innerWidth <= 1024) {
       agentCard.style.position = '';
       agentCard.style.top = '';
       agentCard.style.right = '';
       return;
    }

    const cardHeight = agentCard.offsetHeight;
    const bottomMargin = 10; // Margin between form and the boundary
    const scrollY = window.scrollY;
    
    // Calculate minimum top position based on price tag
    const priceRect = priceTag.getBoundingClientRect();
    const priceBottom = priceRect.bottom;
    
    const header = document.querySelector('.pr-de-header'); 
    const headerHeight = header ? header.offsetHeight : 20;
    
    // The form should start below the price tag with a small gap
    const gapBelowPrice = 20;
    const minFixedTop = Math.max(headerHeight + 20, priceBottom + gapBelowPrice);
    
    // Priority: Stop at footer
    let stopBoundary = null;
    
    // Check footer
    if (footer) {
        const footerRect = footer.getBoundingClientRect();
        const footerDocTop = footerRect.top + scrollY;
        const formBottomIfFixed = scrollY + minFixedTop + cardHeight + bottomMargin;
        
        if (formBottomIfFixed >= footerDocTop) {
            stopBoundary = {
                element: footer,
                docTop: footerDocTop
            };
        }
    }

    if (stopBoundary) {
      // Switch to absolute positioning - form stops here
      const wrapperRect = wrapperWhole.getBoundingClientRect();
      const wrapperDocTop = wrapperRect.top + scrollY;
      
      // Calculate absolute top position to stop before the boundary
      const absoluteTop = stopBoundary.docTop - wrapperDocTop - cardHeight - bottomMargin;
      
      agentCard.style.position = 'absolute';
      agentCard.style.top = absoluteTop + 'px';
      agentCard.style.right = '60px'; 
    } else {
      // Keep fixed - form scrolls with page
      agentCard.style.position = 'fixed';
      agentCard.style.top = minFixedTop + 'px';
      agentCard.style.right = '60px';
    }
  }
  
  window.addEventListener('scroll', checkPosition);
  window.addEventListener('resize', checkPosition);
  checkPosition();
}

if (document.readyState === 'loading') {
  document.addEventListener('DOMContentLoaded', stopFormBeforeFooter);
} else {
  stopFormBeforeFooter();
}
</script>

 <script>
        const sliderTrack = document.getElementById('sliderTrack');
        const nextBtn = document.getElementById('nextBtn');
        const prevBtn = document.getElementById('prevBtn');
        const navDots = document.getElementById('navDots');
        const cards = document.querySelectorAll('.pr-de-property-card');
        
        let currentIndex = 0;
        const totalCards = cards.length;
        
        // Create navigation dots
        for (let i = 0; i < totalCards; i++) {
            const dot = document.createElement('button');
            dot.classList.add('pr-de-dot-item');
            if (i === 0) dot.classList.add('pr-de-current');
            dot.addEventListener('click', () => goToSlide(i));
            navDots.appendChild(dot);
        }
        
        const dots = document.querySelectorAll('.pr-de-dot-item');
        
        function getCardsPerView() {
            const containerWidth = document.querySelector('.pr-de-scroll-container').offsetWidth - 120;
            const cardWidth = 350;
            const gap = 30;
            return Math.floor((containerWidth + gap) / (cardWidth + gap));
        }
        
        function updateCarousel() {
            const cardWidth = 350;
            const gap = 30;
            const offset = currentIndex * (cardWidth + gap);
            sliderTrack.style.transform = `translateX(-${offset}px)`;
            
            // Update dots
            dots.forEach((dot, index) => {
                dot.classList.toggle('pr-de-current', index === currentIndex);
            });
            
            // Calculate max index based on visible cards
            const cardsPerView = getCardsPerView();
            const maxIndex = Math.max(0, totalCards - cardsPerView);
            
            // Hide/Show buttons based on position
            if (currentIndex === 0) {
                prevBtn.classList.add('pr-de-hidden');
            } else {
                prevBtn.classList.remove('pr-de-hidden');
            }
            
            if (currentIndex >= maxIndex) {
                nextBtn.classList.add('pr-de-hidden');
            } else {
                nextBtn.classList.remove('pr-de-hidden');
            }
        }
        
        function goToSlide(index) {
            const cardsPerView = getCardsPerView();
            const maxIndex = Math.max(0, totalCards - cardsPerView);
            currentIndex = Math.min(index, maxIndex);
            updateCarousel();
        }
        
        nextBtn.addEventListener('click', () => {
            const cardsPerView = getCardsPerView();
            const maxIndex = Math.max(0, totalCards - cardsPerView);
            if (currentIndex < maxIndex) {
                currentIndex++;
                updateCarousel();
            }
        });
        
        prevBtn.addEventListener('click', () => {
            if (currentIndex > 0) {
                currentIndex--;
                updateCarousel();
            }
        });
        
        // Handle window resize
        let resizeTimeout;
        window.addEventListener('resize', () => {
            clearTimeout(resizeTimeout);
            resizeTimeout = setTimeout(() => {
                const cardsPerView = getCardsPerView();
                const maxIndex = Math.max(0, totalCards - cardsPerView);
                currentIndex = Math.min(currentIndex, maxIndex);
                updateCarousel();
            }, 100);
        });
        
        // Initialize
        updateCarousel();
    </script>




