    <script>
        const trackElement = document.getElementById('sliderTrack');
        const dotElements = document.querySelectorAll('.pr-de-dot-item');
        
        function moveNext() {
            trackElement.scrollBy({
                left: 410,
                behavior: 'smooth'
            });
        }

        trackElement.addEventListener('scroll', () => {
            const scrollRatio = (trackElement.scrollLeft / (trackElement.scrollWidth - trackElement.clientWidth));
            const currentIndex = Math.round(scrollRatio * (dotElements.length - 1));
            
            dotElements.forEach((dot, idx) => {
                if (idx === currentIndex) {
                    dot.classList.add('pr-de-current');
