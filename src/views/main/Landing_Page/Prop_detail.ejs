            dot.addEventListener('click', () => {
                const scrollValue = (trackElement.scrollWidth - trackElement.clientWidth) * (idx / (dotElements.length - 1));
                trackElement.scrollTo({
                    left: scrollValue,
                    behavior: 'smooth'
                });
            });
        });
    </script>

