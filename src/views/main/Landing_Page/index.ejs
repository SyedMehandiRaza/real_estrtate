       <script>
        // Smooth horizontal scroll for slider
        const sliderContainer = document.querySelector('.slider-container');
        let isDown = false;
        let startX;
        let scrollLeft;

        sliderContainer.addEventListener('mousedown', (e) => {
            isDown = true;
            startX = e.pageX - sliderContainer.offsetLeft;
            scrollLeft = sliderContainer.scrollLeft;
        });

        sliderContainer.addEventListener('mouseleave', () => {
            isDown = false;
        });

        sliderContainer.addEventListener('mouseup', () => {
            isDown = false;
        });

        sliderContainer.addEventListener('mousemove', (e) => {
            if (!isDown) return;
            e.preventDefault();
            const x = e.pageX - sliderContainer.offsetLeft;
            const walk = (x - startX) * 2;
            sliderContainer.scrollLeft = scrollLeft - walk;
        });

        // Navigation arrows functionality
        document.querySelectorAll('.nav-arrow').forEach(btn => {
            btn.addEventListener('click', function(e) {
                e.stopPropagation();
                console.log('Image navigation clicked');
            });
        });
    </script>

     <script>
        let activeSlide = 0;
        const track = document.getElementById('sliderTrack');
        const dots = document.querySelectorAll('.pagination-dot');
        const slideCount = 3;

        function refreshSlider() {
            const cardWidth = document.querySelector('.profile-card').offsetWidth + 30;
            track.style.transform = `translateX(-${activeSlide * cardWidth}px)`;
            
            dots.forEach((dot, idx) => {
                dot.classList.toggle('is-active', idx === activeSlide);
            });
        }

        function moveToNext() {
            activeSlide = (activeSlide + 1) % slideCount;
            refreshSlider();
        }

        function moveToPrev() {
            activeSlide = (activeSlide - 1 + slideCount) % slideCount;
            refreshSlider();
        }

        function jumpToSlide(idx) {
            activeSlide = idx;
            refreshSlider();
        }

        window.addEventListener('resize', refreshSlider);
    </script>




    <!-- search filter implementation js code  -->
    <script>
      document.getElementById("searchBtn").addEventListener("click", async () => {
        const city = document.getElementById("city").value;
        const type = document.getElementById("type").value;
        const purpose = document.getElementById("purpose").value;
        const keyword = document.getElementById("keyword").value;

        const params = new URLSearchParams();

        if (city) params.append("city", city);
        if (type) params.append("type", type);
        if (purpose) params.append("purpose", purpose);
        if (keyword) params.append("keyword", keyword);

        try {
          const res = await fetch(`/search?${params.toString()}`);
          const data = await res.json();

          console.log("Search Results:", data);
          showResults(data);
        } catch (error) {
          console.error("Search error:", error);
        }
      });

      function showResults(properties) {
  const grid = document.querySelector(".properties-grid");
  grid.innerHTML = "";

  if (!Array.isArray(properties) || properties.length === 0) {
    grid.innerHTML = `<p style="color:white;">No properties found</p>`;
    return;
  }

  properties.forEach(p => {
    const thumbnail = p.media?.find(m => m.type === "IMAGE")?.url 
      || "/assets/house-placeholder.webp";

    grid.innerHTML += `
      <div class="property-card">
        <div class="card-image-container">
          <img src="${thumbnail}" alt="${p.title}" class="card-image">

          <div class="card-badges">
            <span class="badge featured">Featured</span>
            <span class="badge">For Sale</span>
          </div>

          <div class="card-nav">
            <button class="nav-btn">‹</button>
            <button class="nav-btn">›</button>
          </div>
        </div>

        <div class="card-content">
          <h3 class="property-title">${p.title}</h3>

          <div class="property-location">
            <svg class="spec-icon" viewBox="0 0 24 24">
              <path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"></path>
              <circle cx="12" cy="10" r="3"></circle>
            </svg>

            <span style="color: lightskyblue; font-weight: bold;">
              ${p.address}
            </span>
          </div>

          <p class="property-type">${p.constructionStatus}</p>

          <div class="property-details">
            <div class="property-price">
              ₹${Number(p.price).toLocaleString("en-IN")}
            </div>
          </div>

          <div class="card-footer">
            <span class="added-date">
              Added: ${new Date(p.createdAt).toDateString()}
            </span>

            <a href="/properties/${p.id}" class="view-detail-btn">
              View detail
            </a>
          </div>
        </div>
      </div>
    `;
  });
}

</script>

