<script>
    const container = document.querySelector(".ts-testimonials-container");
    const cards = document.querySelectorAll(".ts-testimonial-card");
    const prevBtn = document.querySelector(".ts-prev");
    const nextBtn = document.querySelector(".ts-next");

    let index = 0;
    const totalCards = cards.length;
    const cardWidth = cards[0].offsetWidth + 30; // card width + gap

    function updateSlider() {
        container.style.transform = `translateX(-${index * cardWidth}px)`;
        container.style.transition = "0.4s ease";
    }

    nextBtn.addEventListener("click", () => {
        if (index < totalCards - 1) {
            index++;
        } else {
            index = 0; // loop back to start
        }
        updateSlider();
    });

    prevBtn.addEventListener("click", () => {
        if (index > 0) {
            index--;
        } else {
            index = totalCards - 1; // go to last slide
        }
        updateSlider();
    });

    // Optional: Auto Slide
    setInterval(() => {
        index = (index + 1) % totalCards;
        updateSlider();
    }, 4000);
</script>

