    <section class="properties-section">
        <div class="section-header">
            <div class="header-left">
                <h2 class="section-title">
                    Discover The Most<br>Recent Properties
                    <span class="arrow-decoration"><img src="/assets/Vector.png"></span>
                </h2>
            </div>
            <div class="header-right">
                <p class="section-description">
                    Explore our handpicked selection of newly launched properties thoughtfully designed to suit every lifestyle.
                </p>
                <button class="properties-btn">
                    Properties
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <path d="M5 12h14M12 5l7 7-7 7"/>
                    </svg>
                </button>
            </div>
        </div>
        
        <div class="properties-grid">

  <% if (properties && properties.length) { %>
    <% properties.forEach(property => { %>

      <div class="property-card">
        <div class="card-image-container">
          <img 
            src="<%= property.thumbnail || '/assets/house-placeholder.webp' %>" 
            alt="<%= property.title %>" 
            class="card-image"
          >

          <div class="card-badges">
            <span class="badge featured">Featured</span>
            <span class="badge">For Sale</span>
          </div>

          <div class="card-nav">
            <button class="nav-btn">‹</button>
            <button class="nav-btn">›</button>
          </div>
        </div>

        <div class="card-content">
          <h3 class="property-title"><%= property.title %></h3>

          <div class="property-location">
            <svg class="spec-icon" viewBox="0 0 24 24">
              <path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"></path>
              <circle cx="12" cy="10" r="3"></circle>
            </svg>

            <span style="color: lightskyblue; font-weight: bold;">
              <%= property.address %>
            </span>
          </div>

          <p class="property-type">
            <%= property.constructionStatus %>
          </p>

          <div class="property-details">
            <div class="property-price">
              ₹<%= Number(property.price).toLocaleString("en-IN") %>
            </div>
          </div>

          <div class="card-footer">
            <span class="added-date">
              Added: <%= new Date(property.createdAt).toDateString() %>
            </span>

            <a href="/properties/<%= property.id %>" class="view-detail-btn">
              View detail
            </a>
            <!-- <a href="/properties/<%= property.id %>" class="view-detail-btn">
                View detail
            </a> -->

          </div>
        </div>
      </div>

    <% }) %>
  <% } else { %>
    <p style="color:white;">No properties found</p>
  <% } %>

</div>
   
    </section>

